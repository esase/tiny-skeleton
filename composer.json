{
  "name": "esase/skeleton",
  "type": "library",
  "description": "The \"Tiny\" framework's skeleton implementation.\n ",
  "license": "MIT",
  "keywords": [
    "esase",
    "tinyframework",
    "microservices",
    "framework",
    "lightweight",
    "skeleton"
  ],
  "support": {
  },
   "authors": [
        {
            "name": "Alex Ermashev",
            "email": "alexermashev@gmail.com"
        }
    ],
  "require": {
    "php": "^7.4 || ^8.0",
    "esase/tiny-router": "^1.0",
    "esase/tiny-service-manager": "^1.0",
    "esase/tiny-event-manager": "^1.0",
    "esase/tiny-http": "^1.1",
    "esase/tiny-view": "^1.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "google/cloud-translate": "^1.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0 || ^10.0",
    "php-coveralls/php-coveralls": "^2.2",
    "php-mock/php-mock-phpunit": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Tiny\\Skeleton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TinyTest\\Skeleton\\": "test/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
